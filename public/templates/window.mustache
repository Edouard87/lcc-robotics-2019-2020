        <div class="header-item-wrapper header-left">
        <div delTarget="{{wid}}" class="close">

        </div>
        </div>
        <div class="header-item-wrapper header-centered">
        <div class="window-icon">

        </div>
        <h1>{{window_name}}</h1>
        </div>
        <div class="header-item-wrapper header-right">
        <div class="header-right">
        </div>
        </div>
    </div>
        <iframe class="window-content" style="height: {{content_height}}px" meta_1="{{meta_1}}" meta_2="{{meta_2}}" meta_3="{{meta_3}}" meta_4="{{meta_4}}" src="{{page_index}}" frameborder="0"></iframe>
</div>